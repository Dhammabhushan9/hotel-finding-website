<% layout("layout/boilerplate") %>




<body>
    <div>
<h3><b>Start your journey with us</b></h3>

    <div class=" row row-cols-lg-4 row-cols-md-2 row-cols-sm-1 ">
        
        <% for(let list of alllist){ %>
           
            <a href="/listing/<%= list._id %>"class="listing-a" >        
            <div class="card col card-shadow" style="width: 20rem;">
                <img class="card-img-top" style="height: 20rem;" src="<%= list.image %>" alt="Card image cap">

                <div class="card-body">
                    <p class="card-text"><b><%= list.title %></b></p>
                        <p>RS:<%= list.price  %>/night</p>
                </div>
            </div>
            
        <% } %>
    </a>
    </div>
    </div>
</body>
